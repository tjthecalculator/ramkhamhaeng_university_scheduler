        "express": "^4.17.1",
        "mongoose": "^5.10.9",
        "nodemon": "^2.0.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "^4.0.3",
        "react-router-dom": "^5.2.0"
    },
    "devDependencies": {
        "concurrently": "^5.3.0",
        "nodemon": "^2.0.4"
    }
